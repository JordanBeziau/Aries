{% extends "base.html.twig" %}
{% block body %}
  <div class="container">
    {% for message in app.flashes("success") %}
      <div class="alert">
        <span class="alert alert-success">
          {{ message }}
        </span>
      </div>
    {% endfor %}
    <div class="row justify-content">
      <div class="col-10">
        {{ form_start(form) }}
        {{ form_widget(form) }}
        {{ form_end(form) }}
      </div>
    </div>
  </div>
{% endblock %}